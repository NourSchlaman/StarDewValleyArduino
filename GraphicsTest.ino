const int chickenRunningArrayLenght = 4;
const unsigned char* chickenRunningArray[4] = {
	chickenRunning0,
	chickenRunning1,
	chickenRunning2,
	chickenRunning3
};




int counterNeutral = 0;
int counterDead = 0;
int counterEating = 0;

int hunger = 100;
int depression = 0;
bool isEating = false;
bool isRunning = false;
bool isDead = false;

const int button1Pin = 2;
const int button2Pin = 3;
int button1State = LOW;
int button2State = LOW;
int lastButton1State = 0;
int lastButton2State = 0;
float button1Counter = 0;
float button2Counter = 0;

int hungerBalkHeight = 8;
int hungerBalkWidth = 100;

int depressionBalkHeight = u8g2.getHeight();
int depressionBalkWidth = 4;

void setup(void) {
  pinMode(button1Pin, INPUT_PULLUP);
  pinMode(button2Pin, INPUT_PULLUP);

  u8g2.begin(); 
  Serial.begin(9600);
}

void drawChickenNeutral() {
  delay(100);
  u8g2.clearBuffer();	
  u8g2.setDrawColor(1);
  u8g2.drawXBMP(39, 3, 48, 48, chickenNeutralArray[counterNeutral]);
  counterNeutral = (counterNeutral + 1) % 4;
  drawHungerBoxDecreasing();
  drawDepressionBoxIncreasing();
}

void drawChickenRunning() {
  delay(1);
  u8g2.clearBuffer();	
  u8g2.setDrawColor(1);
  u8g2.drawXBMP(39, 3, 48, 48, chickenRunningArray[counterNeutral]);
  counterNeutral = (counterNeutral + 1) % 4;
  drawHungerBoxDecreasing();
  drawDepressionBoxDecreasing();
}

void drawChickenEating() {
  delay(100);
  u8g2.clearBuffer();	
  u8g2.setDrawColor(1);
  u8g2.drawXBMP(39, 3, 48, 48, chickenEatingArray[counterEating]);
  counterEating = (counterEating + 1) % 4;
  drawHungerBoxIncreasing();
  drawDepressionBoxIncreasing();
}

void drawChickenDead() {
  delay(1000);
  u8g2.clearBuffer();	
  u8g2.setDrawColor(1);
  u8g2.drawXBMP(39, 3, 48, 48, chickenDeadArray[counterDead]);
  u8g2.sendBuffer();
  counterDead = (counterDead + 1) % 2;
}

void drawHungerBoxDecreasing(){
  int balkX = (u8g2.getWidth() - hungerBalkWidth) / 2;
  int balkY = u8g2.getHeight() - 5;
  int hungerBalkLength = map(hunger, 0, 100, 0, hungerBalkWidth);

  u8g2.setDrawColor(1);
  u8g2.drawBox(balkX, balkY, hungerBalkLength, hungerBalkHeight);
  u8g2.sendBuffer();
  hunger -= 1;
  if (hunger <= 0) {
      isDead = true;
      drawChickenDead();
  }
}

void drawHungerBoxIncreasing(){
  int balkX = (u8g2.getWidth() - hungerBalkWidth) / 2;
  int balkY = u8g2.getHeight() - 5;
  int hungerBalkLength = map(hunger, 0, 100, 0, hungerBalkWidth);
  u8g2.setDrawColor(1);
  u8g2.drawBox(balkX, balkY, hungerBalkLength, hungerBalkHeight);
  u8g2.sendBuffer();
  hunger += 2;
  if (hunger >= 100) {
        isEating = false;
  }
}

void drawDepressionBoxDecreasing() {
  int balkX = 5;
  int balkY = map(depression, 0, 100, 0, depressionBalkHeight); 
  int depressionBalkLength = depressionBalkHeight; 
  u8g2.setDrawColor(1); 
  u8g2.drawBox(balkX, balkY, depressionBalkWidth, depressionBalkLength); 
  u8g2.sendBuffer();
  depression -= 1;
  if (depression < 0) {
    depression = 0;
  }
}

void drawDepressionBoxIncreasing() {
  int balkX = 5;   
  int balkY = map(depression, 0, 100, 0, depressionBalkHeight); 
  int depressionBalkLength = depressionBalkHeight;
  u8g2.setDrawColor(1); 
  u8g2.drawBox(balkX, balkY, depressionBalkWidth, depressionBalkLength); 
  u8g2.sendBuffer();
  depression += 1;
  Serial.println(depression);
  if (depression >= 100) {
    isDead = true;
  }
}

void loop(void) {
  if (isDead) {
    drawChickenDead();
  } else if (isEating) {
    drawChickenEating();
  } else if (isRunning) {
    drawChickenRunning();
  } else {
    drawChickenNeutral();
  }
  

  button1State = digitalRead(button1Pin);
  button2State = digitalRead(button2Pin);


  if (button1State != lastButton1State) {
    lastButton1State = button1State;
    
    if (button1State == LOW) {
      button1Counter += 1;
    
      if ((int)button1Counter % 2 == 0) {
        isEating = true;
      } else {
        isEating = true;
      }
    }
  }


  if (button2State != lastButton2State) {
    lastButton2State = button2State;
    
    if (button2State == LOW) {
      button2Counter += 1;
    
      if ((int)button2Counter % 2 == 0) {
        isRunning = true;

      } else {
        isRunning = false;

      }
    }
  }
}






