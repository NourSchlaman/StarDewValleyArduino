void setup(void) {
  pinMode(buttonPin, INPUT_PULLUP);
  u8g2.begin(); 
  Serial.begin(9600);
}

void loop(void) {
  u8g2.clearBuffer();	
  u8g2.setDrawColor(1);
  u8g2.drawXBMP(39, 3, 48, 48, chickenNeutralArray[counter]);
    
  int balkHoogte = 8;
  int balkBreedte = 100;
  int balkX = (u8g2.getWidth() - balkBreedte) / 2;
  int balkY = u8g2.getHeight() - 5;

  int balkLengte = map(honger, 0, 100, 0, balkBreedte);

  u8g2.setDrawColor(1);
  u8g2.drawBox(balkX, balkY, balkBreedte, balkHoogte);

  u8g2.setDrawColor(0);
  u8g2.drawBox(balkX, balkY, balkLengte, balkHoogte);

  u8g2.sendBuffer();

  honger += 1;

  if (honger >= 100) {
        honger = 0;
  }

    buttonState = digitalRead(buttonPin);

  if (buttonState != lastButtonState) {
    if (buttonState == HIGH) {
      ledCounter += 0.5;
    } else {
      ledCounter += 0.5;
    }

  }
  if ((int)ledCounter % 2 == 0) {
      Serial.println("AAN");
  } else {
      Serial.println("UIT");
  }
    lastButtonState = buttonState;

  counter = (counter + 1) % 4;
  delay(100);
}


